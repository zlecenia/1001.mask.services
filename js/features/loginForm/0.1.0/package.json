{
  "name": "loginForm",
  "version": "1.0.0",
  "description": "Formularz logowania z walidacją i wirtualną klawiaturą dla ekranu dotykowego 7.9 cala",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run loginForm.spec.js",
    "test:watch": "vitest watch loginForm.spec.js"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "vitest": "^1.0.0"
  },
  "moduleMetadata": {
    "contracts": [],
    "rollbackConditions": {
      "errorRate": ">5%",
      "testFailures": ">0"
    },
    "generated": {
      "prompt": "Formularz logowania z walidacją i wirtualną klawiaturą dla ekranu dotykowego 7.9 cala",
