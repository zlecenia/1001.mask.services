        "touch-and-hold"
      ]
    },
    "authentication": {
      "supportedRoles": [
        "OPERATOR",
        "ADMIN",
        "SUPERUSER",
        "SERWISANT"
      ],
      "minPasswordLength": 3,
      "minUsernameLength": 3,
      "roleBasedAccess": true,
      "passwordValidation": true
    }
  },
  "maskservice": {
    "component": {
      "name": "loginForm",
      "version": "0.1.0",
      "configPath": "./config/"
    }
  }
